[color]
    ui = true
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[user]
	name = Fredrik Gr√∂nqvist
  email = fjnsson@gmail.com
[branch]
       autosetupmerge = true
[push]
       default = current
[core]
  editor = vim
  excludesfile = /Users/fg/.gitignore
[alias]
    co = checkout
    st = status
    ci = commit
    br = branch
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    locate = "!f() { git ls-tree -r --name-only HEAD | grep --color -i -E $1 - ; } ; f"
    branch-dates = "!f() { git for-each-ref --sort=committerdate refs/${1:-heads}/ --format='%(committerdate:short)\t%(authorname)\t%(refname:short)' | sed 's_origin/__g' | column -t -s'      ' ; } ; f"
[url "git@github.com:"]
	insteadOf = https://github.com/
